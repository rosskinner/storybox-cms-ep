{
  "kind": "collectionType",
  "collectionName": "stories",
  "info": {
    "name": "collection",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "credit": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "video": {
      "type": "string",
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)",
      "required": false
    },
    "category": {
      "via": "stories",
      "model": "category"
    },
    "location": {
      "model": "location",
      "via": "stories"
    }
  }
}
