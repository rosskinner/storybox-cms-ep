{
  "kind": "collectionType",
  "collectionName": "stories",
  "info": {
    "name": "collection",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "credit": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "location": {
      "model": "location",
      "via": "stories"
    },
    "media": {
      "type": "dynamiczone",
      "components": [
        "media.link",
        "media.image"
      ]
    },
    "video": {
      "type": "string"
    },
    "articles": {
      "collection": "article",
      "via": "collectionItem"
    },
    "categories": {
      "via": "stories",
      "collection": "category"
    }
  }
}
